{
  "name": "@platform/template-js-rethinkdb-mocked",
  "version": "0.1.0",
  "description": "database mocking used by template projects",
  "main": "template-js-rethinkdb-mocked.dist.js",
  "browser": "template-js-rethinkdb-mocked.dist.js",
  "module": "template-js-rethinkdb-mocked.js",
  "repository": "code.venom360.com/platform/template-js-rethinkdb-mocked",
  "license": "UNLICENSED",
  "publishConfig": {
      "@platform:registry": "https://code.venom360.com/api/v4/projects/554/packages/npm/"
  },
  "scripts": {
    "test": "yarn lint && yarn unit-test",
    "lint": "eslint . && yarn -s lint-yaml",
    "lint-yaml": "command -v yamllint &> /dev/null && yamllint . || echo 'If you want YAML to be linted, install yamllint on your machine.'",
    "lint-fix": "eslint --fix .",
    "unit-test-coverage": "nyc ava",
    "build-dist": "./node_modules/.bin/babel template-js-rethinkdb-mocked.js --out-file template-js-rethinkdb-mocked.dist.js --presets=@babel/env --source-maps inline --copy-files",
    "unit-test": "ava template-js-rethinkdb-mocked.spec.js"
  },
  "dependencies": {
  },
  "devDependencies": {
    "ava": "^3.8.1",
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",      
    "ndb": "^1.1.5",
    "esm": "^3.2.25",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-ava": "^11.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "js-yaml": "^3.13.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.1",
    "nyc": "^15.1.0",
    "sinon": "^9.0.2"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "nyc": {
    "compact": false,
    "include": [
      "template-js-rethinkdb-mocked.js"
    ],
    "files": [
      "template-js-rethinkdb-mocked.spec.js"
    ],
    "require": [
      "esm"
    ],
    "all": true,
    "reporter": "text-summary",
    "cache": true
  },
  "eslintIgnore": [
      "node_modules",
      "build",
      ".nyc_output",
      "coverage",
      ".yarn"
  ],
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": 3,
          "targets": {
            "node": true
          }
        }
      ]
    ]
  }
}
